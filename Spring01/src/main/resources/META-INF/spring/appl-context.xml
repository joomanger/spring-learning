	<bean id="shaDigest" class="com.isd.MessageDigestBeanFactory" p:algorithm="SHA1"/>
	<bean id="md5Digest" class="com.isd.MessageDigestBeanFactory" p:algorithm="MD5"/>
	

	<context:annotation-config />
	<context:component-scan base-package="com.isd" />
